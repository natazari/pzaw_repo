<%- include("head.partial.ejs") %>
<main>
  <h1><%= title %></h1>
  <ul id="collections">
    <% collections.forEach(function(collection) { %>
      <% if (collection.genre === title) { %>
        <li>
          <a href="/collections/<%= collection.id %>">
            <%= collection.artist %>
          </a>
        </li>
      <% } %>
    <% }) %>
  </ul>
</main>
<%- include("foot.partial.ejs") %>